{
    "name": "sllh/styleci-fixers",
    "description": "Auto configure PHP-CS-Fixer from StyleCI config file",
    "keywords": ["PHP-CS-Fixer", "StyleCI", "configuration"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0"
    },
    "require-dev": {
        "php": "^5.6 || ^7.0",
        "styleci/sdk": "^1.0",
        "symfony/console": "^2.3 || ^3.0",
        "twig/twig": "^1.23",
        "sllh/php-cs-fixer-styleci-bridge": "^1.3"
    },
    "autoload": {
        "psr-4": { "SLLH\\StyleCIFixers\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\StyleCIFixers\\Tests\\": "src" }
    }
}
